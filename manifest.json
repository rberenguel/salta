{
  "manifest_version": 3,
  "name": "Salta",
  "description": "Jump tabs, potentially",
  "version": "0.1.0",
  "permissions": ["tabs", "activeTab", "storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "src/popup.html",
    "min_width": 0,
    "max_width": 0
  },
  "commands": {
    "jump": {
      "suggested_key": {
        "default": "Ctrl+Space",
        "mac": "MacCtrl+Space"
      },
      "description": "Jump tabs, potentially"
    }
  },
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  }
}
